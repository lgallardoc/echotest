# EchoTest - Configuración de Ejemplo
# Copie este archivo a .env y ajuste los valores según su entorno

# Configuración del Servidor
PORT=6020
LOG_LEVEL=debug

# Configuración del Cliente (valores por defecto)
CLIENT_IP=127.0.0.1
CLIENT_PORT=6020
CLIENT_ITERATIONS=1
CLIENT_THREADS=1
CLIENT_DELAY=0

# Configuración de Logging
LOG_DIR=log
TMP_DIR=tmp

# Configuración de ISO 8583
ISO8583_MTI_REQUEST=0800
ISO8583_MTI_RESPONSE=0810
ISO8583_RESPONSE_CODE=00
ISO8583_NETWORK_CODE=301 